      <xsd:complexType name="Email_Address_Information_DataType">
        <xsd:annotation>
          <xsd:documentation>Email Address Information</xsd:documentation>
          <xsd:appinfo>
            <wd:Validation>
              <xsd:documentation>Invalid Usage Type and Use For combination for Email Address.</xsd:documentation>
